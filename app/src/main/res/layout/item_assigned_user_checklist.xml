                    android:layout_height="wrap_content"
                    android:text="1st Floor"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Main Columns"
                    android:textColor="@color/colorTextLight"/>
